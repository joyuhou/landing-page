iteId = urlParams.get('sub1') || urlParams.get('site_id') || 'default';\r\n            let adTitle = urlParams.get('utm_content') || urlParams.get('ad_title') || 'default';\r\n            let assetId = urlParams.get('utm_term') || urlParams.get('asset_id') || 'default';\r\n            return { trackingId, siteId, adTitle, assetId };\r\n        }\r\n        function replaceParameterInText(text, paramName, paramValue) {\r\n            const patterns = [\r\n                `\\\\$\\\\{${paramName}\\\\}`,\r\n                `%24%7B${paramName}%7D`,\r\n                `%24%7b${paramName.toLowerCase()}%7d`\r\n            ];\r\n            patterns.forEach(pattern => {\r\n                const regex = new RegExp(pattern, 'gi');\r\n                text = text.replace(regex, paramValue);\r\n            });\r\n            return text;\r\n        }\r\n        function getTrackingIdFromUrl() {\r\n            const urlParams = new URLSearchParams(window.location.search);\r\n            let trackingId = urlParams.get('sub2');\r\n            if (!trackingId && window.location.hash) {\r\n                const hashParams = new URLSearchParams(window.location.hash.substring(1));\r\n                trackingId = hashParams.get('sub2');\r\n            }\r\n            if (!trackingId) {\r\n                const patterns = [\/[?&]sub2=([^&]*)\/i, \/[?&]sub2%3D([^&]*)\/i];\r\n                for (const pattern of patterns) {\r\n                    const match = window.location.href.match(pattern);\r\n                    if (match) {\r\n                        trackingId = decodeURIComponent(match[1]);\r\n                        break;\r\n                    }\r\n                }\r\n            }\r\n            return trackingId || '';\r\n        }\r\n        function replaceAllParameters() {\r\n            const params = getUrlParameters();\r\n            const trackingId = params.trackingId || getTrackingIdFromUrl();\r\n            if (!trackingId) return;\r\n            document.querySelectorAll('*').forEach(function(element) {\r\n                if (element.hasAttribute('href')) {\r\n                    let href = element.getAttribute('href');\r\n                    href = replaceParameterInText(href, 'TRACKING_ID', trackingId);\r\n                    href = replaceParameterInText(href, 'SITE_ID', params.siteId);\r\n                    href = replaceParameterInText(href, 'AD_TITLE', params.adTitle);\r\n                    href = replaceParameterInText(href, 'ASSET_ID', params.assetId);\r\n                    element.setAttribute('href', href);\r\n                }\r\n                if (element.hasAttribute('onclick')) {\r\n                    let onclick = element.getAttribute('onclick');\r\n                    onclick = replaceParameterInText(onclick, 'TRACKING_ID', trackingId);\r\n                    element.setAttribute('onclick', onclick);\r\n                }\r\n            });\r\n        }\r\n        function submitInsuranceForm(event) {\r\n            event.preventDefault();\r\n            const formData = new FormData(event.target);\r\n            const params = getUrlParameters();\r\n            const trackingId = params.trackingId || getTrackingIdFromUrl();\r\n            let redirectUrl = 'https:\/\/www.jyqye.com\/TPSQQP1J\/XKGHXZL6\/?source_id=rimo&sub1=${CAMPAIGN_ID}&sub2=${TRACKING_ID}&sub3=${CAMPAIGN_NAME}&sub4=${PUBLISHER_NAME}&sub5=${SITE_ID}';\r\n            redirectUrl = replaceParameterInText(redirectUrl, 'TRACKING_ID', trackingId);\r\n            redirectUrl = replaceParameterInText(redirectUrl, 'SITE_ID', params.siteId);\r\n            const zipCode = formData.get('zip_code');\r\n            if (zipCode) {\r\n                redirectUrl += '&zip=' + encodeURIComponent(zipCode);\r\n            }\r\n            window.open(redirectUrl, '_blank');\r\n        }\r\n        \/\/ Call setCurrentDate when page loads\r\n        document.addEventListener('DOMContentLoaded', function() {\r\n            setCurrentDate();\r\n            replaceAllParameters();\r\n            let attempts = 0;\r\n            const intervalId = setInterval(function() {\r\n                attempts++;\r\n                replaceAllParameters();\r\n                if (attempts >= 10) {\r\n                    clearInterval(intervalId);\r\n                }\r\n            }, 1000);\r\n            document.addEventListener('click', function(event) {\r\n                const element = event.target.closest('a, button');\r\n                if (element && element.hasAttribute('href')) {\r\n                    const trackingId = getTrackingIdFromUrl();\r\n                    if (trackingId) {\r\n                        let href = element.getAttribute('href');\r\n                        href = replaceParameterInText(href, 'TRACKING_ID', trackingId);\r\n                        element.setAttribute('href', href);\r\n                    }\r\n                }\r\n            });\r\n        });\r\n        \/\/ Smooth scrolling for anchor links\r\n        document.querySelectorAll('a[href^=\"#\"]').forEach(anchor => {\r\n            anchor.addEventListener('click', function (e) {\r\n                e.preventDefault();\r\n                const target = document.querySelector(this.getAttribute('href'));\r\n                if (target) {\r\n                    target.scrollIntoView({\r\n                        behavior: 'smooth'\r\n                    });\r\n                }\r\n            });\r\n        });\r\n        \/\/ Add some interactivity to buttons\r\n        document.querySelectorAll('.cursor-pointer').forEach(button => {\r\n            button.addEventListener('mouseenter', function() {\r\n                this.style.transform = 'scale(1.02)';\r\n                this.style.transition = 'transform 0.2s ease';\r\n            });\r\n            button.addEventListener('mouseleave', function() {\r\n                this.style.transform = 'scale(1)';\r\n            });\r\n        });\r\n        \/\/ Log success message\r\n        console.log('BuyCare Insurance Clone page with hero image loaded successfully!');\r\n    <\/script><\/body><grammarly-desktop-integration data-grammarly-shadow-root=\"true\"><\/grammarly-desktop-integration><\/html>\n","protected":false},"excerpt":{"rendered":"BuyCare &#8211; Auto Insurance Comparison BuyCare Skip to You [&hellip;]","protected":false},"author":28,"featured_media":0,"comment_status":"closed","ping_status":"open","sticky":false,"template":"","format":"standard","meta":{"footnotes":""},"categories":[],"tags":[],"_links":{"self":[{"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/posts\/5904"}],"collection":[{"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/users\/28"}],"replies":[{"embeddable":true,"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/comments?post=5904"}],"version-history":[{"count":6,"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/posts\/5904\/revisions"}],"predecessor-version":[{"id":5959,"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/posts\/5904\/revisions\/5959"}],"wp:attachment":[{"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/media?parent=5904"}],"wp:term":[{"taxonomy":"category","embeddable":true,"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/categories?post=5904"},{"taxonomy":"post_tag","embeddable":true,"href":"https:\/\/buycare.info\/wp-json\/wp\/v2\/tags?post=5904"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}