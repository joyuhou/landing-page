        function setCurrentDate() {
            const now = new Date();
            const options = { year: 'numeric', month: 'short', day: 'numeric' };
            const formattedDate = now.toLocaleDateString('en-US', options);
            document.getElementById('current-date').textContent = formattedDate;
        }

        // Call setCurrentDate when page loads
        document.addEventListener('DOMContentLoaded', function() {
            setCurrentDate();
            updateTrackingLinks();
        });

        // Function to get tracking ID from URL
        function getTrackingIdFromUrl() {
            const urlParams = new URLSearchParams(window.location.search);
            const trackingId = urlParams.get('sub2') || '';
            return trackingId;
        }

                // Function to update all links with the tracking ID
        function updateTrackingLinks() {
            const trackingId = getTrackingIdFromUrl();
            if (!trackingId) return;

            // Select all links that might contain tracking ID placeholder (both encoded and non-encoded)
            const links = document.querySelectorAll('a[href*="TRACKING_ID"]');

            // Update each link with the actual tracking ID
            links.forEach(link => {
                const currentHref = link.getAttribute('href');
                // Replace both encoded (%24%7BTRACKING_ID%7D) and non-encoded (${TRACKING_ID}) versions
                const updatedHref = currentHref
                    .replace('%24%7BTRACKING_ID%7D', trackingId)
                    .replace('${TRACKING_ID}', trackingId);
                link.setAttribute('href', updatedHref);
            });

            console.log('Updated tracking links with ID:', trackingId);
        }

        // Smooth scrolling for anchor links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth'
                    });
                }
            });
        });

        // Add some interactivity to buttons
        document.querySelectorAll('.cursor-pointer').forEach(button => {
            button.addEventListener('mouseenter', function() {
                this.style.transform = 'scale(1.02)';
                this.style.transition = 'transform 0.2s ease';
            });

            button.addEventListener('mouseleave', function() {
                this.style.transform = 'scale(1)';
            });
        });

